{
	"name": "component-driven-development",
	"version": "0.1.0",
	"private": true,
	"license": "MIT",
	"dependencies": {
		"@reach/router": "^1.2.1",
		"@styled-system/prop-types": "^5.1.1",
		"date-fns": "^1.30.1",
		"formik": "^1.5.8",
		"macro-components": "^2.0.0",
		"polished": "^3.4.1",
		"prop-types": "^15.7.2",
		"react": "^16.9.0",
		"react-dom": "^16.9.0",
		"react-hotkeys-hook": "^1.5.2",
		"react-scripts": "3.1.1",
		"stack-styled": "^3.0.0",
		"styled-components": "^4.3.2",
		"styled-system": "^5.1.1"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"pretest": "eslint src",
		"test": "cross-env CI=true react-scripts test --env=jsdom",
		"test:watch": "react-scripts test --env=jsdom",
		"posttest": "npm run format",
		"format": "prettier --write '**/*.{js,md}'"
	},
	"devDependencies": {
		"babel-plugin-styled-components": "^1.10.6",
		"cross-env": "^5.2.1",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"husky": "^3.0.5",
		"lint-staged": "^9.2.5",
		"prettier": "^1.18.2"
	},
	"lint-staged": {
		"*.{js}": [
			"eslint --fix",
			"git add"
		],
		"*.{js,json,md}": [
			"prettier --write",
			"git add"
		]
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	}
}
